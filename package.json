{
  "name": "@wsk1113/vue-EPui-sk",
  "version": "0.1.0",
  "description": "基于Element-Plus扩展而成的组件库 / a Vue3+ElementPlus based highly popular UI library",
  "main": "src/main.js",
  "keywords": [
    "epmenu",
    "eptable",
    "mypageheader",
    "btngroup",
    "epdialog"
  ],
  "author": "wsk1113",
  "license": "ISC",
  "scripts": {
    "dev": "npx rollup -wc rollup.config.dev.js",
    "build": "rimraf dist && npx rollup -c rollup/rollup.config.js",
